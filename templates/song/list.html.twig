{% extends "layout/admin.html.twig" %}


{% block content %}

    <div class="row">
        <div class="col-lg-12">

            <div class="card card-primary card-outline">
                <div class="card-header">
                    <h5 class="m-0">Featured</h5>
                </div>
                <div class="card-body">

                    {% for song in songs %}

                        <h2>{{ song.title }}</h2> | <a href="{{ path("song.edit",{id: song.id}) }}">Editer</a>

                    {% endfor %}

                </div>
            </div>
        </div>
    </div>

{% endblock %}